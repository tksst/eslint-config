{
  "name": "@tksst/eslint-config",
  "version": "0.0.1-development",
  "description": "My personal ESLint config",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/tksst/eslint-config",
  "repository": "github:tksst/eslint-config",
  "license": "MPL-2.0",
  "author": {
    "name": "Takashi Sato",
    "email": "takashi@tks.st",
    "url": "https://www.tks.st/"
  },
  "main": ".eslintrc.js",
  "scripts": {
    "fix": "run-s fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext js .",
    "lint:prettier": "prettier -l .",
    "test": "npm run lint",
    "prepare": "npm run lint"
  },
  "devDependencies": {
    "@tksst/prettier-config": "1.0.2",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "eslint": "8.13.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "npm": "8.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2"
  },
  "dependencies": {
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-plugin-simple-import-sort": "^7.0.0"
  },
  "engines": {
    "node": ">= 14"
  }
}
