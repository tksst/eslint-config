{
  "name": "@tksst/eslint-config",
  "version": "1.3.0",
  "description": "My personal ESLint config",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/tksst/eslint-config",
  "repository": "github:tksst/eslint-config",
  "license": "MPL-2.0",
  "author": {
    "name": "Takashi Sato",
    "email": "takashi@tks.st",
    "url": "https://www.tks.st/"
  },
  "main": "index.js",
  "scripts": {
    "fix": "run-s fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext js .",
    "lint:prettier": "prettier -l .",
    "test": "run-p test:* lint",
    "test:jest": "jest"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@tksst/prettier-config": "1.0.4",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.17.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "28.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "semantic-release": "19.0.3"
  },
  "dependencies": {
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-plugin-simple-import-sort": "^7.0.0"
  },
  "packageManager": "npm@8.7.0",
  "engines": {
    "node": ">= 14"
  }
}
