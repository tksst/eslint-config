{
  "name": "@tksst/eslint-config",
  "version": "4.0.0",
  "description": "My personal ESLint config",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/tksst/eslint-config",
  "repository": "github:tksst/eslint-config",
  "license": "Apache-2.0",
  "author": {
    "name": "Takashi Sato",
    "email": "takashi@tks.st",
    "url": "https://www.tks.st/"
  },
  "type": "module",
  "exports": {
    "import": "./index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "_:build": "esbuild index.js --format=cjs --outfile=dist/index.cjs --bundle  --packages=external",
    "clean": "rimraf ./dist/",
    "build": "run-s clean _:build",
    "prepack": "pnpm build",
    "lint": "run-p --continue-on-error --print-label lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint --cache --cache-strategy content --cache-location ./node_modules/.cache/eslint/ .",
    "fix": "run-s --continue-on-error fix:*",
    "fix:prettier": "pnpm lint:prettier --write",
    "fix:eslint": "pnpm lint:eslint --fix",
    "test": "run-s test:*",
    "test:jest": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "dependencies": {
    "@eslint/js": "8.36.0",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "globals": "13.20.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/git": "10.0.1",
    "@tksst/prettier-config": "1.1.0",
    "cross-env": "7.0.3",
    "esbuild": "0.17.12",
    "eslint": "8.36.0",
    "jest": "29.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.4",
    "rimraf": "4.4.0",
    "semantic-release": "20.1.3",
    "typescript": "5.0.2"
  },
  "peerDependencies": {
    "eslint": "^8.27.0"
  },
  "packageManager": "pnpm@7.29.3",
  "engines": {
    "node": ">= 12"
  }
}
