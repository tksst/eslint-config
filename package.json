{
  "name": "@tksst/eslint-config",
  "version": "2.3.0",
  "description": "My personal ESLint config",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/tksst/eslint-config",
  "repository": "github:tksst/eslint-config",
  "license": "Apache-2.0",
  "author": {
    "name": "Takashi Sato",
    "email": "takashi@tks.st",
    "url": "https://www.tks.st/"
  },
  "exports": {
    ".": "./index.js",
    "./javascript": "./dist/javascript.json",
    "./typescript": "./dist/typescript.json"
  },
  "scripts": {
    "prebuild": "rimraf ./dist/",
    "build": "node ./src/generate.js",
    "fix": "run-s --continue-on-error fix:*",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "lint": "run-p --continue-on-error --print-label lint:*",
    "lint:prettier": "prettier --check .",
    "prelint:eslint": "npm run build",
    "lint:eslint": "eslint --cache --cache-strategy content --cache-location ./node_modules/.cache/eslint/ --ext js .",
    "pretest": "npm run build",
    "test": "run-p test:*",
    "test:jest": "jest"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@tksst/prettier-config": "1.1.0",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "eslint": "8.22.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "29.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.3"
  },
  "dependencies": {
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-plugin-simple-import-sort": "^7.0.0"
  },
  "packageManager": "npm@8.17.0",
  "engines": {
    "node": ">= 12"
  }
}
