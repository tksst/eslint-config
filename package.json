{
  "name": "@tksst/eslint-config",
  "version": "2.3.0",
  "description": "My personal ESLint config",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/tksst/eslint-config",
  "repository": "github:tksst/eslint-config",
  "license": "Apache-2.0",
  "author": {
    "name": "Takashi Sato",
    "email": "takashi@tks.st",
    "url": "https://www.tks.st/"
  },
  "exports": {
    ".": "./index.js",
    "./javascript": "./dist/javascript.json",
    "./typescript": "./dist/typescript.json"
  },
  "scripts": {
    "_:build": "node ./src/generate.js",
    "_:eslint": "eslint --cache --cache-strategy content --cache-location ./node_modules/.cache/eslint/ --ext js .",
    "_:eslintfix": "pnpm _:eslint --fix",
    "clean": "rimraf ./dist/",
    "build": "run-s clean _:build",
    "lint": "run-p --continue-on-error --print-label lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "run-s build _:eslint",
    "fix": "run-s --continue-on-error fix:*",
    "fix:prettier": "pnpm lint:prettier --write",
    "fix:eslint": "run-s build _:eslintfix",
    "test": "run-s build test:*",
    "test:jest": "jest"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@tksst/prettier-config": "1.1.0",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "8.23.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.4",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "jest": "29.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.5",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-plugin-jest": "^27.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0 || ^8.0.0"
  },
  "packageManager": "pnpm@7.12.2",
  "engines": {
    "node": ">= 12"
  }
}
