{
  "name": "@tksst/eslint-config",
  "version": "9.1.0",
  "description": "My personal ESLint config",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/tksst/eslint-config",
  "repository": "github:tksst/eslint-config",
  "license": "Apache-2.0",
  "author": {
    "name": "Takashi Sato",
    "email": "takashi@tks.st",
    "url": "https://www.tks.st/"
  },
  "type": "module",
  "exports": {
    "import": "./index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "_:build": "esbuild index.js --format=cjs --outfile=dist/index.cjs --bundle  --packages=external",
    "clean": "rimraf ./dist/",
    "build": "run-s clean _:build",
    "prepack": "pnpm build",
    "lint": "run-p --continue-on-error --print-label lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint --cache --cache-strategy content --cache-location ./node_modules/.cache/eslint/ .",
    "fix": "run-s --continue-on-error fix:*",
    "fix:prettier": "pnpm lint:prettier --write",
    "fix:eslint": "pnpm lint:eslint --fix",
    "test": "run-s test:*",
    "test:jest": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "dependencies": {
    "@eslint/js": "8.46.0",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "globals": "13.20.0"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.2",
    "@tksst/prettier-config": "1.1.0",
    "@tksst/project-configs": "4.0.0",
    "cross-env": "7.0.3",
    "esbuild": "0.18.17",
    "eslint": "8.45.0",
    "jest": "29.6.2",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.0",
    "rimraf": "5.0.1",
    "typescript": "5.1.6"
  },
  "peerDependencies": {
    "eslint": "^8.27.0"
  },
  "packageManager": "pnpm@8.6.9",
  "engines": {
    "node": ">= 12"
  }
}
